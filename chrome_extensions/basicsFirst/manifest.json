{
  "name": "Getting started example",
  "version": "1.0",
  "description": "Building an extension",
  "manifest_version": 2,

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "browser_action" : {
    "default_action" : "icon.jpg"
  },

  "background": {
    "scripts": ["background.js"]
  }

  

}
